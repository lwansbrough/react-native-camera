{
  "increment": "conventional:angular",
  "changelogCommand": "conventional-changelog -p angular",
  "safeBump": false,
  "src": {
    "commitMessage": "chore: release %s",
    "tagName": "v%s"
  },
  "npm": {
    "publish": true
  },
  "github": {
    "release": true,
    "releaseName": "v%s"
  }
}
